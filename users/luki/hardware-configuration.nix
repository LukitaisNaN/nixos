    { device = "/dev/disk/by-uuid/CD14-C4A9";
      fsType = "vfat";
      options = [ "fmask=0077" "dmask=0077" ];
    };

  swapDevices =
    [ { device = "/dev/disk/by-uuid/a6f007ed-8473-4b83-b6fb-0d640f18c0a1"; }
